 	<update id="CSPDCP812EMmsMsgInsert" parameterClass ="paramMap">
			INSERT INTO MMS_CONTENTS_INFO 
				 	(	CONT_SEQ
				 		, FILE_CNT
				 		, MMS_BODY
				 		, MMS_SUBJECT
				 	) 
			VALUES (	#CONT_SEQ#
						, 1
						, #SMS_TXT#||'접수번호 : '||(	SELECT	ACP_YR||'-청금-'||RM_STTM_ACP_NO
													FROM	TBCSPDCP811M
													WHERE	STTM_DVSN_CD = #STTM_DVSN_CD#
													AND		ACP_YR = #ACP_YR#
													AND		STTM_ACP_NO = #STTM_ACP_NO#)
						, '감사원 청탁금지법 위반 신고 문자통보'
					)
	</update>
	
