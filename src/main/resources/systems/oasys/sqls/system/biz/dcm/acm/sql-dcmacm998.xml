<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="dcm/acm/998">


	<select id="DCMACM998ENuri1Insert" parameterClass="java.util.Map"	resultClass="resultMap">
		/* dcm.acm.service.impl.DCMACM998EServiceImpl.DCMACM998ENuri1Insert */
		<![CDATA[       
			INSERT INTO MSG_DATA                                                            
				      ( MSG_SEQ	                                                            
				      , CUR_STATE                                                           
				      , REQ_DATE                                                            
				      , CALL_TO                                                             
				      , CALL_FROM                                                           
				      , MSG_TYPE                                                            
				      , SMS_TXT                                                            
				     )                                                                      
				 VALUES                                                                     
				     (                                                                      
				       MSG_DATA_SEQ.NEXTVAL                                                 
				     , 0	                                                                  
				     , SYSDATE  
				     , REPLACE(#strRecvTel#, '-', '')                                                  
				     , REPLACE(#strSendTel#, '-', '')                                                  
				     , 4                                                                    
				     , #strMsgTxt#) 
		]]>
	</select>
	
	<select id="DCMACM998ENuri2Insert" parameterClass="java.util.Map"	resultClass="resultMap">
		/* dcm.acm.service.impl.DCMACM998EServiceImpl.DCMACM998ENuri2Insert */
		<![CDATA[       
			CALL NURI2_MESSAGE_SEND(#strSendTel#, #strRecvTel#, #strMsgTit#, #strMsgTxt#)
		]]>
	</select>
	
</sqlMap> 
