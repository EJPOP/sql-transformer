	<select id="BADGDA003EDfntselect" parameterClass="paramMap"   resultMap="dfntselect">
              SELECT 
                     T1.ORG_CD                                                      /* 기관코드 */
                    ,F_ORG_INFO(T1.ORG_CD,'1')      AS ORG_NM                       /* 기관명 */
                    ,T1.NTF_DVSN_CD                                                 /* 통보구분코드 */
                    ,T1.NNB                                                         /* 통보번호 */
                    ,T1.SRNO                                                        /* 일련번호 */
                    ,T1.DPT_CD                                                      /* 부서코드 */
                    ,T1.YR                                                          /* 년도 */
                    
                    /* 
                    ,NVL( ( SELECT X1.EIN
                              FROM TBBADGDA006M X1
                             WHERE X1.ORG_CD      = T1.ORG_CD
                               AND X1.CON_YR      = T1.CON_YR
                               AND X1.CON_SRNO    = T1.CON_SRNO
                               AND X1.SRNO        = T1.SRNO
                               AND X1.OGN_DVSN_CD = ( SELECT X2.OGN_DVSN_CD
                                                        FROM TBBADGDA001M X2
                                                       WHERE X2.ORG_CD      = T1.ORG_CD
                                                         AND X2.NTF_DVSN_CD = T1.NTF_DVSN_CD
                                                         AND X2.NNB         = T1.NNB
                                                          AND ROWNUM        = 1) ), T1.EIN)
                    */ 
                    ,T1.EIN                                           AS EIN        /* 암호화주민등록번호 */              
                    ,T1.DFNT_NM                                                     /* 관계자명 */
                    ,T1.REL_PEN_OCP_CD                                              /* 관련자직종코드 */
                    ,F_CODE_NM('1000057', T1.REL_PEN_OCP_CD)   AS REL_PEN_OCP_NM    /* 관련자직종명 */
                    ,T1.REL_PEN_RANK_CD                                             /* 관련자직급코드 */
                    ,F_CODE_NM('1000058', T1.REL_PEN_RANK_CD)  AS REL_PEN_RANK_NM   /* 관련자직급명 */
                    ,T1.DSP_AM                                                      /* 처분금액 */
                    ,T1.DSP_DT                                                      /* 처분일 */
                    ,T1.ENFM_TYP_CD                                                 /* 집행유형코드 */
                    ,F_CODE_NM('1000249', T1.ENFM_TYP_CD)      AS ENFM_TYP_NM       /* 집행유형명 */                                       
                    ,T1.REGI_DH                                                     /* 등록일시 */
                    ,T1.REGN_ID                                                     /* 등록자ID */
                    ,T1.CRIM_WRDO_TXT                                               /* 범행비위내용 */
                    ,T1.RFR_MTRL_TXT                                                /* 참고자료내용 */
                    ,T1.ATP_DPT_NM                                                  /* 행위시부서명 */
                    ,T1.PRST_DPT_NM                                                 /* 현재부서명 */
                    ,T1.HND_SBJ                                                     /* 조치사항 */
                    ,T1.PSC_YN                                                      /* 고발여부 */
                    ,T1.PST_NOO                                                     /* 직위직명 */
                    ,T1.MNG_PRD_TXT                                                 /* 관리기간내용 */
                    ,T1.ETC_MNG_CD                                                  /* 기타관리코드 */
                    ,T1.MDF_DT                                                      /* 수정일 */
                    ,T1.MDF_PEN_ID                                                  /* 수정자ID */
                    ,T1.ATP_BLT_ORG_CD                                              /* 행위시소속기관코드 */
                    ,F_ORG_INFO(T1.ATP_BLT_ORG_CD,'1')   AS ATP_BLT_ORG_NM          /* 행위시소속기관명 */
                    ,T1.PRST_BLT_ORG_CD                                             /* 현재소속기관코드 */
                    ,F_ORG_INFO(T1.PRST_BLT_ORG_CD,'1')  AS PRST_BLT_ORG_NM         /* 현재소속기관명 */  
                    ,T1.RANK_NM                                                     /* 직급명 */            
                    ,T1.PSC_LWS_STT_TXT                                             /* 고발소송상황내용 */
                    ,T1.SLF_HNDL_HND_TXT                                            /* 자체처리조치내용 */
                    ,T1.ACNT_NOO                                                    /* 회계직명 */
                    ,T1.PSC_DH_HIS                                                  /* 고발일시내역 */
                    ,T1.WRDO_CD                                                     /* 비위코드 */
                    ,T1.WRDO_NM                          AS WRDO_NM                 /* 비위명 */
                    ,T1.PRST_BLT_ACNT_NOO                                           /* 현재소속회계직명 */
                    ,T1.PRST_BLT_MNG_PRD_TXT                                        /* 현재소속관리기간내용 */
                    ,T1.LOS_PSVT_STT_FUTR_PRPT_TXT                                  /* 손해보전상황장래전망내용 */
                    ,T1.CMD_DT                                                      /* 명령일 */
                    ,T1.PD_DDLN_DT                                                  /* 납부기한일 */
                    ,T1.USL_MNG_TXT                                                 /* 평소관리내용 */
                    ,T1.EVT_NM                                                      /* 사건명 */
                    ,T1.ATP_RANK_NM                                                 /* 행위시직급명  */
                    ,T1.HND_RSLT_TXT                                                /* 조치결과내용 */
                    ,T1.CON_YR                                                      /* 연계년도 */
                    ,T1.CON_SRNO                                                    /* 연계일련번호 */
                    ,T1.CON_RANK_CD_NO                                              /* 연계직급코드번호 */
                    ,T1.CON_RANK_NM                                                 /* 연계직급명 */ 
                    ,T1.FRT_USR_ID                                                  /* 최초사용자ID */
                    ,T1.FNL_USR_ID                                                  /* 최종사용자ID */
                    ,T1.FNL_DEAL_DPT_CD                                             /* 최종거래부서코드 */
                    ,T1.FNL_MNU_ID                                                  /* 최종메뉴ID */
                    ,T1.FRT_REGI_DH                                                 /* 최초등록일시 */      
                    ,T1.FNL_MDF_DH                                                  /* 최종수정일시 */
               FROM TBBADGDA001L  T1                      /* 통보사항관계자내역 */  
              WHERE 1=1
                AND T1.ORG_CD        = #strOrgCd#         /* 기관코드 */
                AND T1.NTF_DVSN_CD   = #strNtfDvsnCd#     /* 통보구분코드 */
                AND T1.NNB           = #strNnb#           /* 통보번호 */
                AND T1.SRNO          = #strSrno#          /* 일련번호 */
              
	</select>

	<!-- 통보사항관계자내역 delete -->
	<update id="BADGDA003EDfntDelete" parameterClass="paramMap">
          DELETE 
            FROM TBBADGDA001L                    /* 통보사항관계자내역 */
           WHERE ORG_CD       = #ORG_CD#         /* 기관코드 */
             AND NTF_DVSN_CD  = #NTF_DVSN_CD#    /* 통보구분코드 */
             AND NNB          = #NNB#            /* 통보번호 */
             AND SRNO         = #SRNO#           /*일련번호*/     
	</update>    	
		
