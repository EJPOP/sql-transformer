	<select id="BADGDA003EMainselect" parameterClass="paramMap" resultClass="resultMap">
              SELECT                  
                     D1.ORG_CD                                                  /* 기관코드 */
                    ,F_ORG_INFO(D1.ORG_CD,'1')    AS ORG_NM                     /* 기관명 */     
                    ,D1.NTF_DVSN_CD                                             /* 통보구분코드 */ 
                    ,D1.YR                                                      /* 년도 */         
                    ,D1.DPT_CD                                                  /* 부서코드 */     
                    ,D1.NNB                                                     /* 통보번호 */     
                    ,D1.ACP_STA_CD                                              /* 접수상태코드 */ 
                    ,D1.ACP_MNG_NO                                              /* 접수관리번호 */     
                    ,D1.ACP_DT                                                  /* 접수일 */           
                    ,D1.DGE1_ORG_ACP_DT                                         /* 1차기관접수일 */    
                    ,D1.BAI_BRDV_CD                                             /* 감사원국과코드 */
                    ,F_DPT_INFO(D1.BAI_BRDV_CD,'1')  AS BAI_BRDV_NM             /* 감사원국과명 */                       
                    ,D1.COPT_ORG_CD                                             /* 소관기관코드 */     
                    ,D1.TIT_NM                                                  /* 제목명 */           
                    ,D1.WRDO_CD                                                 /* 비위코드 */
                    ,D1.WRDO_NM                       AS WRDO_NM                /* 비위코드명 */
                    ,D1.DFNT_CNT                                                /* 관계자수 */         
                    ,D1.NTF_ETC_TXT                                             /* 통보기타내용 */     
                    ,D1.OGN_DVSN_CD                                             /* 발생구분코드 */  
                    ,F_CODE_NM('1000202', D1.OGN_DVSN_CD) AS OGN_DVSN_NM        /* 발생구분명 */
                    ,D1.REGI_DT                                                 /* 등록일 */           
                    ,D1.REGN_ID                                                 /* 등록자ID */         
                    ,D1.HNDL_DT                                                 /* 처리일 */           
                    ,D1.TSK_CAT_CD                                              /* 업무분류코드 */
                    ,F_CODE_NM('1000651', D1.TSK_CAT_CD) AS TSK_CAT_NM          /* 업무분류명 */           
                    ,D1.LOS_AM                                                  /* 손해금액 */         
                    ,D1.RFD_AM                                                  /* 환불금액 */         
                    ,D1.SLF_CPS_AM                                              /* 자체변상금액 */     
                    ,D1.OPM_NO                                                  /* 시행문번호 */       
                    ,D1.COFM_DT                                                 /* 승인일 */           
                    ,D1.CPLT_DT                                                 /* 완결일 */           
                    ,D1.ENF_KND_CD                                              /* 시행종류코드 */     
                    ,D1.SUP_ORG_CD                                              /* 감독기관코드 */  
                    ,F_ORG_INFO(D1.SUP_ORG_CD,'1')  AS  SUP_ORG_NM              /* 감독기관명 */  
                    ,D1.PRAC_ORG_CD                                             /* 실행기관코드 */     
                    ,D1.DOC_TEXT_NO                                             /* 문서텍스트번호 */         
                    ,D1.DOC_DT                                                  /* 문서일 */           
                    ,D1.RCEP_ID                                                 /* 접수자ID */         
                    ,D1.APV_DOC_NO                                              /* 결재문서번호 */     
                    ,D1.HNDL_PEN_NM                                             /* 처리자명 */         
                    ,D1.HNDL_OPIN_CD                                            /* 처리의견코드 */
                    ,F_CODE_NM('1000200', D1.HNDL_OPIN_CD)  AS  HNDL_OPIN_NM    /* 처리의견명 */
                    ,D1.CFM_YN                                                  /* 확인여부 */         
                    ,D1.CFM_DH                                                  /* 확인일시 */         
                    ,D1.MDF_DT                                                  /* 수정일 */           
                    ,D1.MDF_PEN_ID                                              /* 수정자ID */         
                    ,D1.EXAM_RSLT_TXT                                           /* 검토결과내용 */     
                    ,D1.HNDL_PEN_RANK_NM                                        /* 처리자직급명 */                         
                    ,D1.HNDL_STT_TXT                                            /* 처리상황내용 */     
                    ,D1.CMT_SBCN_TXT                                            /* 위원회부의내용 */   
                    ,D1.ENF_DT                                                  /* 시행일 */           
                    ,D1.AUD_MTRL_RMK                                            /* 감사자료비고 */     
                    ,D1.AUD_MTRL_YR                                             /* 감사자료년도 */     
                    ,D1.AUD_MTRL_DPT_CD                                         /* 감사자료부서코드 */ 
                    ,D1.AUD_MTRL_SRNO                                           /* 감사자료일련번호 */ 
                    ,D1.SLF_PRSS_STA_CD                                         /* 자체진행상태코드 */ 
                    ,D1.SLF_SEN_DT                                              /* 자체발송일 */       
                    ,D1.SLF_APV_DT                                              /* 자체결재일 */
					,D1.PNP_NTF_ACP_DT                                          /* 검경통보접수일 */
					,D1.PNP_NTF_RCEP_NM                                         /* 검경통보접수자명 */
					,D1.PNP_NTF_POLICE_DOC_NO                                   /* 검경통보경찰문서번호 */
					,D1.PNP_NTF_POLICE_NM                                       /* 검경통보경찰명 */
					,D1.PNP_NTF_PRSC_DOC_NO                                     /* 검경통보검찰문서번호 */
					,D1.PNP_NTF_PRSC_NM                                         /* 검경통보검찰명 */
					,D1.BAI_RPT_DT                                              /* 감사원보고일 */
					,D1.DTEN_DT                                                 /* 입건일 */
					,D1.PRS_NEWS_DT                                             /* 언론보도일 */
					,D1.LAD_NTF_DVSN_CD                                         /* 손망실통보구분코드 */
					,D1.CRM_NTF_DVSN_CD                                         /* 범죄통보구분코드 */                    
                    ,D1.DOC_ID                                                  /* 문서ID */           
                    ,D1.DOC_TYP_CD                                              /* 문서유형코드 */     
                    ,D1.FRT_USR_ID                                              /* 최초사용자ID */     
                    ,D1.FNL_USR_ID                                              /* 최종사용자ID */     
                    ,D1.FNL_DEAL_DPT_CD                                         /* 최종거래부서코드 */ 
                    ,D1.FNL_MNU_ID                                              /* 최종메뉴ID */       
                    ,D1.FRT_REGI_DH                                             /* 최초등록일시 */     
                    ,D1.FNL_MDF_DH                                              /* 최종수정일시 */     
                    ,D1.CON_RET_KY_NM                                           /* 연계반려키명 */     
                    ,D1.CON_YR                                                  /* 연계년도 */         
                    ,D1.CON_SRNO                                                /* 연계일련번호 */
               FROM TBBADGDA001M  D1     /* 통보기본사항 */
              WHERE 1=1        
                AND D1.ORG_CD      = #strOrgCd#
                AND D1.NTF_DVSN_CD = #strNtfDvsnCd#
                
                <!-- 
                AND D1.YR          = #strYr#
                AND D1.DPT_CD      = #strDptCd#
                 -->
                 
                AND D1.NNB         = #strNnb#
	</select>

