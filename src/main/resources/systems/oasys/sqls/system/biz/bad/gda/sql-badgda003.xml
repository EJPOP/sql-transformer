	<select id="BADGDA003EMaxSrnoSelect" parameterClass="paramMap" resultClass="resultMap">
        <![CDATA[     	
                  SELECT TO_CHAR(NVL(MAX(SRNO),0) + 1) AS MAX_SRNO
                    FROM TBBADGDA001L
                   WHERE ORG_CD      = #ORG_CD#
                     AND NTF_DVSN_CD = #NTF_DVSN_CD#
                     AND NNB         = #NNB#
		]]>
		
	</select>	
