			<result property="acpYr" 						 column="ACP_YR" 						     jdbcType="VARCHAR" javaType="java.lang.String" />
			<result property="sttmAcpNo" 			 column="STTM_ACP_NO" 			     jdbcType="VARCHAR" javaType="java.lang.String" />
