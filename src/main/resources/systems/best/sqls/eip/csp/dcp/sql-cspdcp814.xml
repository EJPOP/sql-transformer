			<result property="emlAd" 						 column="EML_AD" 						     jdbcType="VARCHAR" javaType="java.lang.String" />
			<result property="titNm" 						 column="TIT_NM" 						     jdbcType="VARCHAR" javaType="java.lang.String" />
