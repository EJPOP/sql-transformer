<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.go.bai.eip.TEST123.dao.testTMapper">
    
    <!-- 금융정보제출요구 조회 -->
    <select id="selectMntyInfSmtRqList" parameterType="paramMap" resultType="egovMap" >
    /*kr.go.bai.eip.aep.mac.dao.AEPMAC001QMapper.selectMntyInfSmtRqList*/
        SELECT T1.NO1
        		,T1.ID1
        		,T1.NAME1
        		FROM TEST1 T1
    </select>
    
    <insert id="insertTest" parameterType="paramMap">
        /*kr.go.bai.eip.aep.mac.dao.AEPMAC001PMapper.TBBADDED108MInsert*/
        /*증거서류함 Insert*/
        INSERT INTO TEST1
        (
            NO1
            ,ID1
            ,NAME1
        )
        values
        (
            (SELECT NVL(MAX(NO1),0)+1 FROM TEST1 T1)
            ,#{id1}  /*최초사용자id */
            ,#{name1} /*최종사용자id */
        )
    </insert>
    
    <update id="updateTest" parameterType="paramMap">
    	UPDATE TEST1 T1
    		SET T1.ID1 = #{id1}
    		,T1.NAME1 = #{name1}
    	WHERE NO1 = #{no1}
    </update>
    
   
   <delete id="delTest" parameterType="paramMap">
   		DELETE TEST1 T1
   		WHERE T1.NO1 = #{no1}
   </delete>
</mapper> 
