<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.go.bai.eip.bad.ded.dao.BADDED013PMapper">
    
    <select id="selectList" parameterType="paramMap" resultType="egovMap">
    /*kr.go.bai.eip.bad.ded.dao.BADDED013PMapper.selectList*/
        <include refid="include.pageSelct" />
        SELECT CD AS RGN_CD,
               CD_NM AS RGN_NM
          FROM TBDCMACM013C
         WHERE USE_YN = 'Y'
           AND CMM_CD_DVSN_CD = '1000001'
           AND CD != '000000000'
           <if test="schRgnCd != null and schRgnCd != ''">
           AND CD LIKE '%'||#{schRgnCd}||'%'
           </if>
           <if test="schRgnNm != null and schRgnNm != ''">
           AND CD_NM LIKE '%'||#{schRgnNm}||'%'
           </if>
        <include refid="include.pageOrder" />
    </select>
    
</mapper>